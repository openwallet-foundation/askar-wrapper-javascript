  "dependencies": {
    "@openwallet-foundation/askar-shared": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.1.03",
    "nitrogen": "*",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-nitro-modules": "*",
    "typescript": "catalog:"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "*"
  },
  "binary": {
    "version": "v0.5.0",
    "host": "https://github.com/openwallet-foundation/askar/releases/download",
